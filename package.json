{
  "name": "discogs-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2",
    "@types/react-redux": "^7.1.24",
    "disconnect": "1.2.2",
    "global": "4.4.0",
    "moment": "2.29.3",
    "nodemon": "2.0.15",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "ts-node": "^10.7.0",
    "underscore": "1.13.3"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "sass": "sass src/Sass:src/Css --watch --no-source-map",
    "dev": "react-scripts start | yarn run sass",
    "start": "react-scripts start && yarn run watch-css",
    "server": "nodemon app/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "restart": "yarn build-css && yarn build && yarn start",
    "clean": "rm -rf package-lock.json && rm -rf yarn.lock && rm -rf node_modules && rm -rf build/ && npm i && npm run build"
  },
  "proxy": "http://localhost:9000/",
  "devDependencies": {
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/react-router-dom": "^5.3.3",
    "babel-preset-env": "1.7.0",
    "node-sass": "7.0.1",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "16.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
