{
  "name": "discogs-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "disconnect": "1.2.2",
    "global": "4.4.0",
    "moment": "2.29.1",
    "nodemon": "2.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "underscore": "1.13.2",
    "why-did-you-update": "1.0.8"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "dev": "react-scripts start | yarn run watch-css",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "restart": "yarn build-css && yarn build && yarn start"
  },
  "proxy": "http://127.0.0.1:3001",
  "devDependencies": {
    "babel-preset-env": "1.7.0",
    "node-sass": "7.0.1"
  },
  "engines": {
    "node": "17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
