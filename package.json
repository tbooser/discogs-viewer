{
  "name": "discogs-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "disconnect": "1.2.2",
    "global": "4.4.0",
    "moment": "2.29.1",
    "nodemon": "2.0.15",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "typescript": "^4.6.2",
    "underscore": "1.13.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "dev": "react-scripts start | yarn run watch-css",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "restart": "yarn build-css && yarn build && yarn start",
    "clean": "rm -rf package-lock.json && rm -rf yarn.lock && rm -rf node_modules && rm -rf build/ && npm i && npm run build"
  },
  "proxy": "http://127.0.0.1:3001",
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "babel-preset-env": "1.7.0",
    "node-sass": "7.0.1"
  },
  "engines": {
    "node": "16.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
